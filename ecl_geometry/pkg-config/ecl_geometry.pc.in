prefix=@CMAKE_INSTALL_PREFIX@
exec_prefix=${prefix}

Name: ecl_geometry
Description: Embedded control libraries, mathematical geometry library.
Version: @PROJECT_VERSION@
URL: http://code.google.com/p/embedded-control-library/

Requires: ecl_license, ecl_build, ecl_config, ecl_containers, ecl_formatters, ecl_linear_algebra, ecl_exceptions, ecl_math, ecl_mpl, ecl_type_traits
includedir=${prefix}/include
libdir=${exec_prefix}/lib
Libs: -L${libdir} -lecl_geometry
Cflags: -I${includedir}
